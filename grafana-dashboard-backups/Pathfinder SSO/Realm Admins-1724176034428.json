          "rawSql": "SELECT\r\n  \"public\".\"keycloak_role\".\"id\" AS \"id\",\r\n  \"public\".\"keycloak_role\".\"client_role\" AS \"client_role\",\r\n  \"public\".\"keycloak_role\".\"description\" AS \"description\",\r\n  \"public\".\"keycloak_role\".\"name\" AS \"name\",\r\n  \"public\".\"keycloak_role\".\"realm_id\" AS \"realm_id\",\r\n  \"public\".\"keycloak_role\".\"realm\" AS \"realm\",\r\n  \"User Entity - User\".\"id\" AS \"User Entity - User__id\",\r\n  \"User Entity - User\".\"email\" AS \"User Entity - User__email\",\r\n  \"User Entity - User\".\"email_constraint\" AS \"User Entity - User__email_constraint\",\r\n  \"User Entity - User\".\"email_verified\" AS \"User Entity - User__email_verified\",\r\n  \"User Entity - User\".\"enabled\" AS \"User Entity - User__enabled\",\r\n  \"User Entity - User\".\"federation_link\" AS \"User Entity - User__federation_link\",\r\n  \"User Entity - User\".\"first_name\" AS \"User Entity - User__first_name\",\r\n  \"User Entity - User\".\"last_name\" AS \"User Entity - User__last_name\",\r\n  \"User Entity - User\".\"realm_id\" AS \"User Entity - User__realm_id\",\r\n  \"User Entity - User\".\"username\" AS \"User Entity - User__username\",\r\n  \"User Entity - User\".\"created_timestamp\" AS \"User Entity - User__created_timestamp\",\r\n  \"User Entity - User\".\"service_account_client_link\" AS \"User Entity - User__service_account_client_link\",\r\n  \"User Entity - User\".\"not_before\" AS \"User Entity - User__not_before\"\r\nFROM\r\n  \"public\".\"keycloak_role\"\r\n \r\nLEFT JOIN \"public\".\"user_role_mapping\" AS \"User Role Mapping\" ON \"public\".\"keycloak_role\".\"id\" = \"User Role Mapping\".\"role_id\"\r\n  LEFT JOIN \"public\".\"user_entity\" AS \"User Entity - User\" ON \"User Role Mapping\".\"user_id\" = \"User Entity - User\".\"id\"\r\nWHERE\r\n  (\r\n    (\"public\".\"keycloak_role\".\"realm_id\" <> 'master')\r\n   \r\n    OR (\"public\".\"keycloak_role\".\"realm_id\" IS NULL)\r\n  )\r\n \r\n   AND (\"public\".\"keycloak_role\".\"name\" = 'realm-admin')\r\n  AND (\"User Entity - User\".\"email\" IS NOT NULL)\r\n  AND (\r\n    (\"User Entity - User\".\"email\" <> '')\r\n    OR (\"User Entity - User\".\"email\" IS NULL)\r\n  )\r\nORDER BY\r\n  \"public\".\"keycloak_role\".\"realm_id\" ASC\r\nLIMIT\r\n  1048575",
