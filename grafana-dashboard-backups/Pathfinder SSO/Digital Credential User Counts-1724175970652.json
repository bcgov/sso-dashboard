          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \r\nkeycloak_role.name AS \"Standard Client\",\r\nCOUNT(federated_identity.federated_username) AS \"Number of Role Users\"\r\nFROM federated_identity\r\nINNER JOIN user_role_mapping ON (federated_identity.user_id = user_role_mapping.user_id)\r\nINNER JOIN keycloak_role ON (user_role_mapping.role_id = keycloak_role.id)\r\nWHERE federated_identity.realm_id = 'standard'\r\nAND federated_identity.identity_provider = 'digitalcredential'\r\nAND keycloak_role.name != 'default-roles-standard'\r\nGROUP BY keycloak_role.name\r\nORDER BY COUNT(federated_identity.federated_username)",
